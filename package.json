{
    "name": "rails-latest-data-migration",
    "displayName": "rails-latest-data-migration",
    "description": "Opens the latest data migration created in a Ruby on Rails app.",
    "version": "0.0.1",
    "publisher": "alextakitani",
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.railsLatestDataMigration"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "extension.railsLatestDataMigration",
            "title": "Rails Latest Data Migration"
        }]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "license": "MIT"
}
